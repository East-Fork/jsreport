{
  "name": "@jsreport/jsreport-data",
  "version": "3.0.0-beta.1",
  "description": "jsreport extension adding support for using sample data for previewing templates",
  "keywords": [
    "jsreport",
    "data"
  ],
  "homepage": "https://github.com/jsreport/jsreport-data",
  "bugs": {
    "url": "https://github.com/jsreport/jsreport-data/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport-data.git"
  },
  "license": "LGPL",
  "author": {
    "name": "Jan Blaha"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout 5000 && standard"
  },
  "dependencies": {},
  "devDependencies": {
    "handlebars": "4.7.7",
    "in-publish": "2.0.1",
    "@jsreport/jsreport-core": "3.0.0-beta.1",
    "@jsreport/jsreport-handlebars": "3.0.0-beta.1",
    "@jsreport/jsreport-studio-dev": "3.0.0-beta.1",
    "mocha": "6.1.4",
    "should": "13.2.3",
    "standard": "16.0.3"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}