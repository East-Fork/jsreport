{
  "name": "@jsreport/jsreport-scripts",
  "version": "3.0.0-beta.1",
  "description": "jsreport extension capable of running custom javascript functions during the rendering process",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport/tree/master/packages/jsreport-scripts",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport.git"
  },
  "license": "LGPL",
  "author": "Jan Blaha",
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "start": "jsreport-studio-start --httpPort 4300",
    "test": "mocha test --timeout 7000 && standard"
  },
  "dependencies": {
    "lodash.omit": "4.5.0",
    "node.extend.without.arrays": "1.1.6"
  },
  "devDependencies": {
    "bluebird": "3.5.4",
    "in-publish": "2.0.1",
    "@jsreport/jsreport-assets": "3.0.0-beta.1",
    "@jsreport/jsreport-core": "3.0.0-beta.1",
    "@jsreport/jsreport-jsrender": "3.0.0-beta.1",
    "@jsreport/studio-dev": "3.0.0-beta.1",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "16.0.3"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}