{
  "name": "@jsreport/jsreport-version-control",
  "version": "3.0.0-beta.1",
  "description": "jsreport extension adding support for versioning templates and other entities",
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "homepage": "https://github.com/jsreport/jsreport-version-control",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-version-control.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "main": "index.js",
  "files": [
    "lib",
    "studio",
    "test",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "set DEBUG=jsreport && mocha test --timeout 5000 && standard"
  },
  "dependencies": {
    "bluebird": "3.5.4",
    "bytes": "3.0.0",
    "diff": "3.5.0",
    "diff-match-patch": "1.0.5",
    "isbinaryfile": "3.0.3",
    "lodash.omit": "4.5.0",
    "mime-types": "2.1.27",
    "nanoid": "3.1.22",
    "node.extend.without.arrays": "1.1.6"
  },
  "devDependencies": {
    "in-publish": "2.0.1",
    "@jsreport/jsreport-assets": "3.0.0-beta.1",
    "@jsreport/jsreport-authentication": "3.0.0-beta.1",
    "@jsreport/jsreport-chrome-pdf": "3.0.0-beta.1",
    "@jsreport/jsreport-core": "3.0.0-beta.1",
    "@jsreport/jsreport-data": "3.0.0-beta.1",
    "@jsreport/jsreport-express": "3.0.0-beta.1",
    "@jsreport/studio-dev": "3.0.0-beta.1",
    "mocha": "5.2.0",
    "puppeteer": "9.1.1",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "16.0.3",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}