{
  "name": "jsreport",
  "private": true,
  "description": "javascript based business reporting",
  "keywords": [
    "report",
    "business",
    "javascript"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "compile": "node scripts/compile.js",
    "performance": "node --expose-gc --max-old-space-size=100 tools/performance.js",
    "start": "node server.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js"
  },
  "resolutions": {},
  "dependencies": {
    "@jsreport/studio-dev": "3.0.1"
  },
  "devDependencies": {
    "archiver": "5.3.0",
    "cross-env": "5.2.1",
    "standard": "16.0.3"
  },
  "standard": {
    "ignore": [
      "data/**"
    ],
    "env": {
      "mocha": true,
      "browser": true,
      "node": true
    }
  }
}